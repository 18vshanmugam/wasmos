{
  "compilerOptions": {
    "lib": ["webworker", "es6", "scripthost"],
    "module": "es2015",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "outDir": "build/",
    /* "target": "ESNext", */
    "strict": false,
    "skipLibCheck": true
  },
  "include": [
    "./worker.ts"
  ],
  "exclude": [
        "node_modules",
        "**/*.spec.ts",
        "src/**/*.ts"
    ]
}
